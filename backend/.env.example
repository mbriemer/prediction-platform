# Example environment variables - DO NOT ADD REAL CREDENTIALS HERE
# Copy this file to .env and fill in the values

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/prediction-platform?retryWrites=true&w=majority

# Environment settings
NODE_ENV=production

# Server settings
PORT=10000

# CORS origin (frontend URL)
CORS_ORIGIN=https://your-frontend-app.onrender.com

# Session secret (use a strong random string in production)
SESSION_SECRET=create-a-secure-random-string-here